{% extends 'base.html.twig' %}

{% block title %}Photo{% endblock %}

{% block body %}
    <h1>Photo</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ photo.id }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ photo.createdAt ? photo.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ photo.updatedAt ? photo.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>GaleryFrontName</th>
                <td>{{ photo.galeryFrontName }}</td>
            </tr>
            <tr>
                <th>GaleryFrontSize</th>
                <td>{{ photo.galeryFrontSize }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_gestapp_photo_index') }}">back to list</a>

    <a href="{{ path('app_gestapp_photo_edit', {'id': photo.id}) }}">edit</a>

    {{ include('gestapp/photo/_delete_form.html.twig') }}
{% endblock %}
